<html>
  <head>
    <script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>
    <script type="text/javascript">
      google.charts.load('current', {'packages':['corechart']});
      google.charts.setOnLoadCallback(drawVisualization);

      function drawVisualization() {
        // Some raw data (not necessarily accurate)
        var data = google.visualization.arrayToDataTable([
          ['Warehouse', 'Rice', 'Maize', 'Wheat', 'Corn', 'Millet(Bajra)', 'Average' ],
          ['1'        ,  165  ,  938,      522,    998,       450,          614.6   ],
          ['2'        ,  135  ,  1120,     599,    1268,      288,            682   ],
          ['3'        ,  157  ,  1167,     587,    807,       397,            623   ],
          ['4'        ,  139  ,  1110,     615,    968,       215,          609.4   ],
          ['5'        ,  136  ,  691,      629,    1026,      366,          569.6   ]
        ]);

        var options = {
          title : 'Capacity of Wearhouse in Ton',
          vAxis: {title: 'Quantity(kg)'},
          hAxis: {title: 'Warehouses'},
          seriesType: 'bars',
          series: {5: {type: 'line'}}
        };

        var chart = new google.visualization.ComboChart(document.getElementById('chart_div'));
        chart.draw(data, options);
      }
    </script>
  </head>
  <body>
    <h1>Statistics About Legumes Warehouses</h1>
    <nav aria-label="breadcrumb">
        <ol class="breadcrumb">
          <li class="breadcrumb-item"><a href="/">Home</a></li>
          <li class="breadcrumb-item"><a href="/dashboard">Dashboard</a></li>
          <li class="breadcrumb-item active" aria-current="page">Legumes Wearhouse Stats</li>
        </ol>
      </nav>
    <div id="chart_div" style="width: 900px; height: 500px;"></div>
  </body>
</html>
